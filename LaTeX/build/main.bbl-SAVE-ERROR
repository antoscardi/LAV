      \strng{namehash}{ebb6f86bb233338397b717dae51dc45e}
      \strng{fullhash}{818994e88ad3c5fbc63c8dfb4cb39ec5}
      \strng{fullhashraw}{818994e88ad3c5fbc63c8dfb4cb39ec5}
      \strng{bibnamehash}{818994e88ad3c5fbc63c8dfb4cb39ec5}
